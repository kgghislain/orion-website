<%# Needs: contentSections = ContentSection[]%>

<div id="id-content"
    style="
        display: flex;
        flex-direction: column;
        align-items: center;
    ">
    <% contentSections.forEach( function (contentSection) { %>
        <%- include('content-section/contentSection', {contentSection: contentSection}) %>
    <% }) %>
</div>
