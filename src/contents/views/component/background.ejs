<% background.backgrounds.forEach( function (bg) { %>
    <img 
        class="background"

        <% if (bg.img) { %>src="<%= bg.img%>"<% } %>

        style="
            width: 100%; height: 100%;
            position: fixed; top: 0%; z-index: -10;
            opacity: 1;
            <% if (bg.fadingTime) { %> transition: opacity <%= bg.fadingTime%>s; <% } %>
    ">
<% } ) %>
